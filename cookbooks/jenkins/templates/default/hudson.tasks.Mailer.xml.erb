<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.4">
  <defaultSuffix><%= @suffix -%></defaultSuffix>
  <smtpAuthUsername><%= @username -%></smtpAuthUsername>
  <smtpAuthPassword><%= @password -%></smtpAuthPassword>
  <replyToAddress><%= @replyTo -%></replyToAddress>
  <smtpHost><%= @smtp_host -%></smtpHost>
  <useSsl><%= @smtp_ssl -%></useSsl>
  <smtpPort><%= @smtp_port -%></smtpPort>
  <charset><%= @smtp_charset -%></charset>
</hudson.tasks.Mailer_-DescriptorImpl>